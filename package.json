{
  "name": "duolingo-memo",
  "private": true,
  "version": "0.1.0",
  "description": "A browser extension to remember your answers in Duolingo",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "test:e2e": "vitest",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatiasDuhalde/duolingo-memo.git"
  },
  "keywords": [
    "chrome",
    "firefox",
    "edge",
    "duolingo",
    "extension"
  ],
  "author": "Mat√≠as Duhalde <matias.duhalde@uc.cl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MatiasDuhalde/duolingo-memo/issues"
  },
  "homepage": "https://github.com/MatiasDuhalde/duolingo-memo#readme",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/lab": "6.0.0-beta.9",
    "@mui/material": "^6.0.2",
    "radashi": "12.2.0-beta.ef0154b",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.19.0",
    "webextension-polyfill": "^0.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.25",
    "@types/chrome": "^0.0.270",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "prettier": "^3.3.3",
    "puppeteer": "^23.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vitest": "^2.0.5"
  }
